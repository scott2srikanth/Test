  	<url-pattern>/logout</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>CategoryServlet</servlet-name>
  	<url-pattern>/category</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>VegServlet</servlet-name>
  	<url-pattern>/Veg</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>NonVegServlet</servlet-name>
