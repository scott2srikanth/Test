  </servlet>
  <servlet>
  	<servlet-name>AddTodoServlet</servlet-name>
  	<display-name>AddTodoServlet</display-name>
  	<description></description>
  	<servlet-class>com.app.AddTodoServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>LogoutServlet</servlet-name>
  	<display-name>LogoutServlet</display-name>
  	<description></description>
  	<servlet-class>com.app.LogoutServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>CategoryServlet</servlet-name>
  	<display-name>CategoryServlet</display-name>
